pipeline {
    agent any

    stages {
        stage('Hello World') {
            steps {
                mvn clean install
            }
          }
        stage('Build') {
            steps {
               git url: 'https://github.com/cyrille-leclerc/multi-module-maven-project'
    withMaven {
      sh "mvn clean install"
    }
            }
        }
    }
}
